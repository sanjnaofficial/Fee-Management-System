<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#50C878"
    tools:context=".Home3">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="397dp"
        android:layout_height="67dp"
        android:backgroundTint="@color/teal_700"
        android:gravity="center"
        android:text="My Details"
        android:textAlignment="center"
        android:textColor="#32527B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/ll1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.277"
        android:layout_width="408dp"
        android:layout_height="588dp"
        android:backgroundTint="@color/purple_200"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="56dp">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:background="#CDE990"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.542"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_width="match_parent"
            android:layout_height="74dp"
            android:layout_margin="10dp"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="67dp"
                android:layout_height="31dp"
                android:textColor="#0F6292"
                android:layout_margin="30dp"
                android:text="Name" />

            <TextView
                android:id="@+id/nameTV"
                android:textColor="#0F6292"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:text="Sanjna" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:background="#CDE990"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.285"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_width="match_parent"
            android:layout_height="74dp"
            android:layout_margin="10dp"
            android:layout_marginBottom="28dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:textColor="#0F6292"
                android:layout_margin="30dp"
                android:text="Registration No." />

            <TextView
                android:id="@+id/regTV"
                android:textColor="#0F6292"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:text="121" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:background="#CDE990"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.514"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_width="match_parent"
            android:layout_height="74dp"
            android:layout_margin="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_margin="30dp"
                android:textColor="#0F6292"

                android:text="Email Id" />

            <TextView
                android:id="@+id/emailTV"
                android:textColor="#0F6292"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:text="sanjnadkds@gmail.com" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:background="#CDE990"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.457"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_width="match_parent"
            android:layout_height="74dp"
            android:layout_margin="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_margin="30dp"
                android:textColor="#0F6292"
                android:text="Phone Number" />

            <TextView
                android:id="@+id/phoneTV"
                android:textColor="#0F6292"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:text="1234567890" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout5"
            android:background="#CDE990"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.457"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_width="match_parent"
            android:layout_height="74dp"
            android:layout_margin="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_margin="30dp"
                android:textColor="#0F6292"
                android:text="Total Fees" />

            <TextView
                android:id="@+id/feesTotalTV"
                android:textColor="#0F6292"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:text="10000" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout6"
            android:background="#CDE990"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.514"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_width="match_parent"
            android:layout_height="74dp"
            android:layout_margin="10dp"
            android:layout_marginBottom="28dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_margin="30dp"
                android:textColor="#0F6292"
                android:text="Pending Fees" />

            <TextView
                android:id="@+id/feesPendingTV"
                android:textColor="#0F6292"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:text="500" />
        </LinearLayout>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>